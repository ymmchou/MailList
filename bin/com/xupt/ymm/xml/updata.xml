		<method name="size" para="120,22"></method>
		<method name="=位置" para="40,40"></method>
	</controller>
	<controller parent="jfrmupdataFrame" id="txtid" type="文本框">
		<method name="font" para="#normalFont"></method>
		<method name="size" para="120,22"></method>
