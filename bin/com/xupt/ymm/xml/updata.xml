			para="'确认'">
		<method name="font" para="#buttonFont"></method>
		<method name="size" para="55,30"></method>
		<method name="=位置" para="50,200"></method>
	</controller>
	<controller parent="jfrmupdataFrame" id="btnout" type="按钮"
			para="'退出'">
