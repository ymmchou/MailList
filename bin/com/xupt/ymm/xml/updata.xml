	</controller>
	<controller parent="jfrmupdataFrame" id="lblname" type="标签"
			para="'姓       名： '">
		<method name="font" para="#normalFont"></method>
		<method name="size" para="120,22"></method>
		<method name="=位置" para="40,80"></method>
	</controller>
	<controller parent="jfrmupdataFrame" id="txtname" type="文本框">
		<method name="font" para="#normalFont"></method>
		<method name="size" para="120,22"></method>
		<method name="=位置" para="150,80"></method>
	</controller>
